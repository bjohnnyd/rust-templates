    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.4",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
